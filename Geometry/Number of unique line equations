#include <bits/stdc++.h>
using namespace std;

//Function to get a normalized line representation between two integer points
pair<pair<ll,ll>, ll> get_line(pt a, pt b) {
    ll dx = b.x - a.x;
    ll dy = b.y - a.y;
    ll A = -dy, B = dx;
    ll g = __gcd(abs(A), abs(B));
    A /= g; B /= g;
    if(A < 0 || (A == 0 && B < 0)) A *= -1, B *= -1;
    ll C = A*a.x + B*a.y;
    return {{A,B}, C};
}

int main() {
    int n; cin >> n;
    vector<pair<int,int>> p(n);
    for(auto& [x,y] : p) cin >> x >> y;
    cout << countLines(p);
}
