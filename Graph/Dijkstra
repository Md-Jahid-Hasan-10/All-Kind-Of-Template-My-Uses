const ll oo = 1e18;
const ll mod = 1e9 + 7;
const ll N = 2e5 + 123;
void dijkstra(int s,int n,vi &dis,vector<vector<pii>> &graph){
    priority_queue<pii> q;
    for(int i = 1; i <= n; i++){
        dis[i]=oo;
    }
    dis[s]=0;
    q.push({0,s});
    while(!q.empty()){
        int a = q.top().S; 
        int co = -q.top().F;
        q.pop();
        if(dis[a] < co)continue;
        for (auto u: graph[a]){
            int b = u.F,w = u.S;
            if (dis[a]+w < dis[b]){
                dis[b] = dis[a]+w;
                q.push({-dis[b],b});
            }
        }
    }
}

void Goriber_solve()
{
    int n,m,s,t,p;
    cin >> n >> m >> s >> t >> p;
    vi dis1(n+1),dis2(n+1);
    vector<vector<pii>> graph1(n+1),graph2(n+1);
    vector<tu> edges;
    for(int i = 1; i <= m; i++){
        int u,v,w;
        cin >> u >> v >> w;
        graph1[u].pb({v,w});
        graph2[v].pb({u,w});
        edges.pb({u,v,w});
    }
    dijkstra(s,n,dis1,graph1);
    dijkstra(t,n,dis2,graph2);
    int ans = -1;
    for(int i = 0; i < m; i++){
        int u = edges[i][0],v = edges[i][1],w = edges[i][2];
        int tot = dis1[u] + dis2[v] + w;
        if(tot <= p){
            ans = max(ans,w);
        }
    }
    cout << ans << endl;
}
signed main()
{
    Fokinnir_moto_kaj_koris_na
    int _ = 1,tc = 0;
    cin >> _;
    while (_--)
    {
        cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
}
