const ll N = 1e6+2;
const ll mod = 1e9 + 7;
const ll oo = 1e18;
struct triplate{
    int x,y,gcd;
};
//! ax + by = gcd(a,b)
triplate extendedEuclid(int a, int b)
{
    if(b == 0)return {1, 0, a};
    triplate smallans = extendedEuclid(b, a % b);
    triplate ans;
    ans.x = smallans.y;
    ans.gcd = smallans.gcd;
    ans.y = (smallans.x - (a / b) * smallans.y);
    return ans;
}
int ex_gcd(int a,int b,int &x,int &y){
    if(b == 0){
        x = 1;
        y = 0;
        return a;
    }
    int x1,y1;
    int ans = ex_gcd(b,a % b,x1,y1);
    x = y1;
    y = x1 - y1 * (a / b);
    return ans;
}
void Goriber_solve()
{
    int a,b;
    cin >> a >> b;
    triplate ans = extendedEuclid(a,b);
    cout << ans.x << " " << ans.y << " " << ans.gcd << endl;
    cout << " GCD is " << ex_gcd(a,b) << endl;
}
signed main()
{
 
    Fokinnir_moto_kaj_koris_na
    int _ = 1,tc = 0;
    //cin >> _;
    while (_--)
    {
        //cout<<"Case "<<++tc<<": ";
        Goriber_solve();
    }
}
